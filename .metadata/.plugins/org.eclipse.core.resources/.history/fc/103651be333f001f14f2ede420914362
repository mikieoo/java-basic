package chapter1;

public class ControlEtc {

	public static void main(String[] args) {

		// continue : 반복문에서 continue를 만나면 조건 검사로 넘어감
		// break : 반복문에서 break를 만나면 반복문을 종료함
		
		// count가 3의 배수가 아니면 count 출력
		// count3
		for (int i = 1; i <= 100; i++) {
			if(i % 3 == 0) continue;
			if(i % 4 == 0 && i % 11 == 0) break;
			System.out.println(i);
		}
	}

}
